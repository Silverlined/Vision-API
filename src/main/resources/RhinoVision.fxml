<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<Pane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <ImageView fx:id="imageView" fitHeight="600.0" fitWidth="400.0" pickOnBounds="true" />
      <Button fx:id="shootButton" layoutX="174.0" layoutY="261.0" mnemonicParsing="false" onAction="#takeAPicture" style="-fx-background-radius: 30; -fx-translate-x: 0;" text="Shoot">
         <font>
            <Font name="Chiller" size="18.0" />
         </font>
      </Button>
      <ImageView fx:id="snapchatLogo" fitHeight="151.0" fitWidth="152.0" layoutX="119.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../Downloads/logo.png" />
         </image>
      </ImageView>
   </children>
</Pane>
